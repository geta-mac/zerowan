<div>
  <ul>
    <li><%= @pet.name %></li>
    <li><%= @pet.age %></li>
    <li><%= @pet.castration %></li>
    <li><%= @pet.category %></li>
    <li><%= @pet.animal_type %></li>
    <li><%= @pet.character %></li>
    <li><%= @pet.gender %></li>
    <li><%= image_tag @pet.image1.url %></li>
    <li><%= image_tag @pet.image2.url %></li>
    <li><%= image_tag @pet.image3.url %></li>
    <li><%= image_tag @pet.image4.url %></li>
    <li><%= @pet.description %></li>
  </ul>
    <%= link_to "編集", edit_pet_path(@pet) %>
    <%= link_to "戻る", pets_path %>
  <br>
    <%= link_to "削除", pet_path(@pet), method: :delete, data: { confirm: '本当に削除してもいいですか?' }  %>
</div>
